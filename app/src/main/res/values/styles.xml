<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="addfood_name">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_title</item>

    </style>

    <style name="addfood_desc">
        <item name="android:textSize">@dimen/text_subhead</item>
        <item name="android:textColor">@color/black</item>

    </style>

    <style name="addfood_label_carbs">
        <item name="android:textSize">@dimen/text_subhead</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>

    </style>

    <style name="addfood_tv_nutrient">
        <item name="android:textSize">@dimen/text_title</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>

    </style>
    <style name="addfood_tv_nutrient_precent">
        <item name="android:textSize">@dimen/text_subhead</item>
        <item name="android:textColor">@color/defaultBottomNav</item>

    </style>

    <style name="addfood_label_proteins">
        <item name="android:textSize">@dimen/text_subhead</item>
        <item name="android:textColor">@color/colorEndColorProgressBar1</item>

    </style>

    <style name="addfood_label_fats">
        <item name="android:textSize">@dimen/text_subhead</item>
        <item name="android:textColor">@color/colorStartColorProgressBar2</item>

    </style>

    <style name="addfood_label_total">
        <item name="android:textSize">@dimen/text_subhead</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>

    </style>

    <style name="addfood_label_enter">
        <item name="android:textSize">@dimen/text_head</item>
        <item name="android:textColor">@color/black</item>

    </style>
    <style name="Theme.StepsAndCaloriesApp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="tv_overview_foodname">
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:textSize">@dimen/text_head</item>
    </style>
    <style name="tv_overview_foodlabel">
        <item name="android:textSize">@dimen/text_subhead</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/grey</item>
    </style>
    <style name="Widget.MaterialComponents.TextInputLayout.OutlinedBox"
        parent="Base.Widget.MaterialComponents.TextInputLayout">
        <item name="materialThemeOverlay">
            @style/ThemeOverlay.MaterialComponents.TextInputEditText.OutlinedBox
        </item>
        <item name="boxCollapsedPaddingTop">0dp</item>
        <item name="android:textColor">@color/white</item>
    </style>



</resources>